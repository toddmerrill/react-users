{
  "name": "react-user-client",
  "version": "1.0.0",
  "description": "React.js implementation of a simple master/detail form",
  "scripts": {
    "clean": "rm -rf build; rm -rf tsBuild;",
    "prestart": "npm run clean",
    "start": "webpack-dev-server --progress --colors --content-base build/",
    "build:bundle": "webpack",
    "prebuild": "npm run clean",
    "build": "npm run build:bundle",
    "ts": "tsc; cp -rf src/style tsBuild/src; cp src/index.html tsBuild/src",
    "tsonly": "npm run clean; npm run ts",
    "test": "jest",
    "tsc": "tsc"
  },
  "babel": {
    "presets": [
      "react",
      "latest",
      "stage-3"
    ]
  },
  "repository": {
    "type": "git",
    "url": "ssh://todd@toddmerrill.com/home/todd/git/react-users"
  },
  "keywords": [
    "react",
    "redux",
    "client",
    "master",
    "detail"
  ],
  "author": "Todd Merrill",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-react": "^6.5.0",
    "bootstrap": "^3.3.7",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-logger": "^2.8.2",
    "redux-thunk": "^2.2.0",
    "users-api": "1.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.2.4",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "mocha": "^3.0.2",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.2",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.3.0",
    "webpack-dev-server": "^2.4.2"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*-test.js"
    ]
  }
}
